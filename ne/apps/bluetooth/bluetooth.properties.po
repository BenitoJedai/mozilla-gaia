#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "ब्लुटुथ"

#: confirmation
msgid "Confirmation"
msgstr "यकिन गर्नुहोस्"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "ब्लुटुथ अक्षम पारिएको छ"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "के तपाई ब्लुटुथ अन गर्न चाहनुहुन्छ?"

#: cancel
msgid "Cancel"
msgstr "रद्द गर्नुहोस्"

#: turn-on
msgid "Turn On"
msgstr "अन गर्नुहोस्"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "ब्लुटुथ मार्फत फाइल स्थानान्तर असफल"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"ब्लुटुथ मार्फत फाइल स्थानान्तर असफलभयो। फाईल स्थानान्तर सफल बनाउन ब्लुटुथ सेटिङ्गहरू "
"मिलाउनुहोस्।"

#: ok
msgid "OK"
msgstr "ठीक छ"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "सम्बन्धन कायम भएका साधनहरु "

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "नजिक रहेका यन्त्रहरु।"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "जडान गर्न छुनुहोस्"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "सम्पर्क स्थापना गर्दै"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "अन्य यन्त्रको जवाफ कुर्दै "

#: device-status-paired
msgid "Paired"
msgstr "सम्पर्क स्थापीत।"

#: device-status-connecting
msgid "Connecting…"
msgstr "सम्पर्क स्थापना गर्दै"

#: device-status-connected
msgid "Connected"
msgstr "सम्पर्क स्थापना गरियो।"

#: search-for-device
msgid "Searching for devices…"
msgstr "यन्त्रहरु खोज्दै।"

#: search-device
msgid "Search for devices"
msgstr "यन्त्रहरु खोज्नुहोस्।"

#: unnamed-device
msgid "Unknown device"
msgstr "अज्ञात यन्त्र"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "यन्त्रहरु बिच सम्पर्क स्थापना हुन सकेन।"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "सम्पर्क स्थापना हुन सकेन। पिन नम्बर जाँच्नुहोस्।"

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "ैधेरै अनुरोधहरुका कारण सम्पर्क स्थापना हुन सकेन।"

#~ msgid "Select Bluetooth Device"
#~ msgstr "ब्लुतुथ साधन छनोट गर्नुहोस"

#~ msgid "No Bluetooth devices paired."
#~ msgstr "कुनै ब्लुटुथ यन्त्रहरु जोडी बनाइएको छैन।"

#~ msgid "Would you like to pair a device now?"
#~ msgstr "के तपाई अहिले अरु यन्त्रसँग जोडी बनाउन चाहनुहुन्छ?"

#~ msgid "Choose your option"
#~ msgstr "तपाईँको विकल्प रोज्नुहोस्"
