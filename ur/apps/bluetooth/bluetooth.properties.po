#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "بلوٹوتھ"

#: confirmation
msgid "Confirmation"
msgstr "تصدیق"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "بلوٹوتھ کو غیر فعال کر دیا ہے"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "کیا آپ بلوٹوت کو چالو کرنا چاہتے ہیں؟"

#: cancel
msgid "Cancel"
msgstr "منسوخ کریں"

#: turn-on
msgid "Turn On"
msgstr "چالو کریں"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "بلوٹوت فائل ٹرانسفر ناکام ہوگئی"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"بلوٹوت فائل ٹرانسفر ناکام ہوگئی ہے. چیک کریں کہ بلوٹوت کی ترتیبات درست ہیں."

#: ok
msgid "OK"
msgstr "ٹھیک ہے"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "جوڑی شدہ آلے"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "اس علاقہ میں آلات"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "رابطہ قائم کرنے کے لئے تھپتھپائیں"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "آلے کے ساتھ جوڑی بنا رہا ہے…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "دوسرے آلے کا انتظار کر رہا ہے…"

#: device-status-paired
msgid "Paired"
msgstr "جوڑا شدہ"

#: device-status-connecting
msgid "Connecting…"
msgstr "جڑ رہا ہے…"

#: device-status-connected
msgid "Connected"
msgstr "كنیكٹ کر گیا ہے"

#: search-for-device
msgid "Searching for devices…"
msgstr "آلات کی تلاش کر رہا ہے…"

#: search-device
msgid "Search for devices"
msgstr "آلات تلاش کریں"

#: unnamed-device
msgid "Unknown device"
msgstr "نامعلوم آلہ"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "آلات جوڑنے میں ناکام"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "آلہ سے جڑنے میں ناکام۔ پڑتال کریں کہ PIN صحیح ہے۔"

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "آلہ سے جڑنے میں ناکام۔ بہت زیادہ درخواست ہیں۔"

#~ msgid "Select Bluetooth Device"
#~ msgstr "بلو ٹوٹھ آلہ منتخب کریں"

#~ msgid "No Bluetooth devices paired."
#~ msgstr "کوی بلو ٹوتھ ڈیوایس تیار نہی۔"

#~ msgid "Would you like to pair a device now?"
#~ msgstr "کیا اپ ڈیوایس کوابھی پیر کرنا چاھتے ہیں؟"

#~ msgid "Choose your option"
#~ msgstr "اپنے اختیار کو منتخب کریں"
