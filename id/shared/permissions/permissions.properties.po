#: perm-contacts
msgid "Contacts"
msgstr "Kontak"

#: perm-contacts-appRequest
msgid "{{app}} would like to access your contact list."
msgstr "{{app}} ingin mengakses daftar kontak Anda."

#: perm-contacts-webRequest
msgid "{{site}} would like to access your contact list."
msgstr "{{site}} ingin mengakses daftar kontak Anda."

#: perm-desktop-notification
msgid "Notifications"
msgstr "Notifikasi"

#: perm-desktop-notification-appRequest
msgid "{{app}} would like to display notifications."
msgstr "{{app}} ingin menampilkan notifikasi."

#: perm-desktop-notification-webRequest
msgid "{{site}} would like to display notifications."
msgstr "{{site}} ingin menampilkan notifikasi."

#: perm-device-storage-music
msgid "Music Storage"
msgstr "Penyimpanan musik"

#: perm-device-storage-music-appRequest
msgid "{{app}} would like to access your music."
msgstr "{{app}} ingin mengakses musik Anda."

#: perm-device-storage-music-webRequest
msgid "{{site}} would like to access your music."
msgstr "{{site}} ingin mengakses musik Anda."

#: perm-device-storage-pictures
msgid "Image Storage"
msgstr "Penyimpanan Gambar"

#: perm-device-storage-pictures-appRequest
msgid "{{app}} would like to access your images."
msgstr "{{app}} ingin mengakses gambar Anda."

#: perm-device-storage-pictures-webRequest
msgid "{{site}} would like to access your images."
msgstr "{{site}} ingin mengakses gambar Anda."

#: perm-device-storage-sdcard
msgid "Memory Card Storage"
msgstr "Penyimpanan Kartu Memori"

#: perm-device-storage-sdcard-appRequest
msgid "{{app}} would like to access your memory card."
msgstr "{{app}} ingin mengakses kartu memori Anda."

#: perm-device-storage-sdcard-webRequest
msgid "{{site}} would like to access your memory card."
msgstr "{{site}} ingin mengakses kartu memori Anda."

#: perm-device-storage-videos
msgid "Video Storage"
msgstr "Penyimpanan Video"

#: perm-device-storage-videos-appRequest
msgid "{{app}} would like to access your videos."
msgstr "{{app}} ingin mengakses video Anda."

#: perm-device-storage-videos-webRequest
msgid "{{site}} would like to access your videos."
msgstr "{{site}} ingin mengakses video Anda."

#: perm-fmradio
msgid "FM Radio"
msgstr "Radio FM"

#: perm-fmradio-appRequest
msgid "{{app}} would like to use the FM Radio."
msgstr "{{app}} ingin mengakses Radio FM."

#: perm-fmradio-webRequest
msgid "{{site}} would like to use the FM Radio."
msgstr "{{site}} ingin mengakses Radio FM."

#: perm-geolocation
msgid "Geolocation"
msgstr "Geolokasi"

#: perm-geolocation-appRequest
msgid "{{app}} would like to know your location."
msgstr "{{app}} ingin mendapatkan lokasi Anda."

#: perm-geolocation-webRequest
msgid "{{site}} would like to know your location."
msgstr "{{site}} ingin mendapatkan lokasi Anda."

#: perm-geolocation-more-info
msgid ""
"{{brandShortName}} uses data such as GPS, Wi-Fi, and mobile networks near "
"you to help determine your approximate location. Data is sent to Mozilla and "
"other service providers and will be used in the aggregate to improve those "
"location databases.  Data may be stored on your device and data collection "
"may occur even when no apps are running."
msgstr ""

