#: firefox-accounts-title
msgid "Firefox Accounts"
msgstr "Awon apamo Firefox"

#: fxa-next
msgid "Next"
msgstr "Omiran"

#: fxa-back
msgid "Back"
msgstr "Pada"

#: fxa-done
msgid "Done"
msgstr "Opari"

#: fxa-ok
msgid "OK"
msgstr "Beeni"

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Errors
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # 'spec' references below refer to the spec attached to bug 960130
#. # this is a real error screen (7.2 in spec)
#: fxa-connection-error-title
msgctxt "fxa-connection-error-title"
msgid "Unable to Connect"
msgstr "Ko sese lati sopo"

#: fxa-connection-error-message
msgid "You can create an account or sign in from Settings."
msgstr "O le seda apamo kan tabi fi amin wole lati inu awon Agbekale"

#. # this is a real error screen (5.4 in spec)
#: fxa-reset-password-error-title
msgid "Reset Password"
msgstr "Tun amin-asiri to"

#: fxa-reset-password-error-message
msgid "You can reset your password from Settings."
msgstr "O le tun amin-asiri re to lati inu awon Agbekale."

#. # this is a real error screen (19.4 in spec)
#: fxa-coppa-failure-error-title
msgid "Cannot Create Account"
msgstr "Kole Seda Apamo"

#: fxa-coppa-failure-error-message
msgid ""
"You must meet certain age requirements to create a Firefox Account. "
"{{learnmore}}"
msgstr "ojo-ori re ni lati wa ni ibi kan "

#. # LOCALIZATION NOTE(fxa-learn-more): This string corresponds to the {{learnmore}}
#. # placeholder in fxa-coppa-failure-error-message.
#: fxa-learn-more
msgid "Learn more"
msgstr "Nimo si"

#. # this is a real error screen (13.2 and 17.2 in spec)
#: fxa-offline-error-title
msgctxt "fxa-offline-error-title"
msgid "Unable to Connect"
msgstr "Ko sese lati sopo"

#: fxa-offline-error-message
msgid "Please connect to a network from Settings."
msgstr "Jowo sopo mo netiwoki lati inu awon Agbekale."

#. # this is a real error screen (3.2 in spec)
#: fxa-invalid-email-title
msgid "Error"
msgstr "Asise"

#: fxa-invalid-email-message
msgid "Please enter a valid email."
msgstr "Jowo fi ero-ifiweranse to fesemule si."

#. # this is a real error screen (5.2 / 11.1 in spec)
#: fxa-invalid-password-title
msgid "Invalid Password"
msgstr "Amin-asiri ti ko fesemule"

#: fxa-invalid-password-message
msgid "Please check your password and try again."
msgstr "Jowo ye amin-asiri re wo ki o si gbiyanju lekansi."

#. # this is the fallback error
#: fxa-unknown-error-title
msgctxt "fxa-unknown-error-title"
msgid "Unable to Connect"
msgstr "Ko sese lati sopo"

#: fxa-unknown-error-message
msgid "System unavailable, try again soon."
msgstr "Eto-ero kosi larowoto, gbiyanju lekansi laipe."

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Interstitial screen
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-connecting
msgid "Connecting"
msgstr "Onsopo"

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Enter Email
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-enter-email-input.placeholder
msgid "Enter your email"
msgstr "Fi ero-ifiranse re si"

#: fxa-use-email-to-sign-in
msgid "Use your email address to create an account or sign in."
msgstr "Lo apejuwe ero-ifiweranse re lati seda apamo tabi fi amin wole"

#: fxa-notice
msgid ""
"By proceeding, I agree to the {{tos}} and {{pn}} of Firefox cloud services."
msgstr ""
"Nipa lilo siwaju, Mo ti gba lati {{tos}} ati {{pn}} ti awon ise Firefox "
"ofurufu."

#. # LOCALIZATION NOTE(fxa-tos): This string corresponds to the {{tos}} placeholder
#. # in fxa-notice.
#: fxa-tos
msgid "Terms of Service"
msgstr "Awon ofin Ise"

#. # LOCALIZATION NOTE(fxa-pn): This string corresponds to the {{pn}} placeholder
#. # in fxa-notice.
#: fxa-pn
msgid "Privacy Notice"
msgstr "Akiyesi Ibi-asiri"

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Set & Enter Password
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-create-password-title
msgid "Create a Password"
msgstr "Seda Amin-asiri"

#: fxa-enter-password-title
msgid "Enter Your Password"
msgstr "Fi Amin-asiri Re si"

#: fxa-hello-user
msgid "Hello, {{email}}"
msgstr "Kaabo, {{ero-ifiweranse}}"

#: fxa-password-refresh
msgid "Please enter your Firefox Account password to continue."
msgstr "Jowo fi amin-asiri Apamo Firefox re si lati tesiwaju."

#: fxa-password.placeholder
msgid "Password"
msgstr "Amin-asiri"

#: fxa-forgot-your-password
msgid "Forgot your password?"
msgstr "O ti gbagbe amin-asiri re?"

#: fxa-show-password
msgid "Show password"
msgstr "Fi amin-asiri han"

#: fxa-password-length-requirement
msgid "Your password must be at least 8 characters long."
msgstr "Amin-asiri re gbodo ni o kere ju awon onkosile 8 ni gigun."

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Signin success
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-success
msgid "Success!"
msgstr "Aseyori!"

#: fxa-ready-to-go
msgid "Your Firefox Account is ready to go! "
msgstr "Apamo Firefox re ti setan lati lo!"

#: fxa-use-features
msgid ""
"Now you can use features like Find My Device and apps like the Firefox "
"Marketplace."
msgstr ""
"Nibayi o le lo awon ohun-elo bi Wa Ohun-amulo Mi ati ohun-eto bi Oju-oja "
"Firefox."

#: fxa-access-manage-account-settings
msgid "In the future, you can access and manage your account from Settings."
msgstr "Ni ojo iwaju, o le laye ati sakoso apamo re lati inu awon Agbekale."

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # Signup success
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-confirm-account
msgid "Confirm Your Account"
msgstr "Sejerisi Apamo Re"

#: fxa-will-send-email
msgid "We will send an email to: {{email}}"
msgstr "A o fi ero-ifiweranse ranse si: {{ero-ifiweranse}}"

#: fxa-signup-click-verification-link-in-email
msgid ""
"Next time you check your email, click the verification link to confirm your "
"Firefox Account."
msgstr ""
"Ni igba miran ti o ba ye ero-ifiweranse re wo, tee itokasi aridaju lati "
"jerisi Apamo Firefox re."

#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#. # COPPA
#. #=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#=#
#: fxa-age-verification
msgid "Age Verification"
msgstr "Aridaju Ojo-ori"

#: fxa-age-1990-earlier
msgid "1990 or earlier"
msgstr "1990 tabi odun seyin"

#: fxa-year-of-birth
msgid "Year of Birth"
msgstr "Odun ti a bii"
