msgstr "Iseto"

#: cancel-button
msgid "Cancel"
msgstr "Pare"

#: download_error
msgid "Download Error"
msgstr "Asise Igbasile"

#: install_error
msgid "Installation Error"
msgstr "Asise Iseto"

#: err_descriptor_download_failed
msgctxt "err_descriptor_download_failed"
msgid "The download failed because of a server error."
msgstr "Igbasile kuna nitori asise olupin."

#: err_bad_descriptor
msgctxt "err_bad_descriptor"
msgid "The download failed because of a server error."
msgstr "Igbasile kuna nitori asise olupin."

#: err_too_big
msgid "File is too large to download."
msgstr "Atojo ti tobi ju lati gbasile"

#: err_unsupported_type
msgid "This type of purchased media is not supported on your phone."
msgstr ""
"Iru igbeohunatiaworanjade ti a ra ko ni atileyin lori ero-ibanisoro re."

#: err_bad_type
msgctxt "err_bad_type"
msgid "The download failed because of a server error."
msgstr "Igbasile kuna nitori asise olupin."

#: err_content_download_failed
msgctxt "err_content_download_failed"
msgid "The download failed because of a server error."
msgstr "Igbasile kuna nitori asise olupin."

#: err_bad_drm_message
msgctxt "err_bad_drm_message"
msgid "The download failed because of a server error."
msgstr "Igbasile kuna nitori asise olupin."

#: err_bad_image
msgid "The downloaded image cannot be displayed on this device."
msgstr "Aworan ti a gbasile ko se fihan lori ohunamulo yi."

#: err_bad_audio
msgid "The downloaded file cannot be played on this device."
msgstr "Atojo ti a gbasile ko le sere ninu ohunamulo yi."

#: err_no_space
msgid "There is not enough storage space on your device to save this song."
msgstr "Ko si aaye ibi-ipamo toto ninu ohunamulo re lati fi orin yi pamo."

#: err_no_sdcard
msgid "Cannot save song because there is no memory card."
msgstr "Ko le fi orin pamo nitoripe ko si kaadi iranti."

#: err_sdcard_in_use
msgid ""
"Cannot save song because the memory card is in use. Unplug your phone and "
"try downloading again."
msgstr ""
"Ko le fi orin pamo nitoripe a nlo kaadi iranti lowo. Yo ero-ibanisoro re "
"kuro ki o si gbiyanju gbigbasile lekansi."

#: err_ds_save_failure
msgctxt "err_ds_save_failure"
msgid "Error saving media file."
msgstr "Asise fifipamo atojo igbeohunatiaworanjade"

#: err_db_store_failure
msgctxt "err_db_store_failure"
msgid "Error saving media file."
msgstr "Asise fifipamo atojo igbeohunatiaworanjade"

#: success_song
msgid "Song downloaded and installed. Use the Music app to play it."
msgstr "A ti gbasile a si ti seto Orin. Lo ohunelo Orin lati fi gbo."

#: success_ringtone
msgid "Ringtone downloaded and installed."
msgstr "A ti gbasile a si ti seto Didun-ohunamulo."

#: success_wallpaper
msgid "Wallpaper downloaded and installed."
msgstr "A ti gbasile a si ti seto aworanogiri."

#: ringtone-or-song-question
msgid "Do you want to install this audio file as a song or as a ringtone?"
msgstr ""
"Se o fe lati seto atojo didunohun yi gege bi orin tabi gege bi didun-"
"ohunamulo?"

