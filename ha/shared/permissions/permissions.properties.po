msgstr "{{app}} na son ya sami dama ga hotunanka"

#: perm-device-storage-pictures-webRequest
msgid "{{site}} would like to access your images."
msgstr "{{site}} na son ya sami dama ga hotunanka"

#: perm-device-storage-sdcard
msgid "Memory Card Storage"
msgstr "Ma'ajiyar Kati Mai ƙwaƙwalwa"

#: perm-device-storage-sdcard-appRequest
msgid "{{app}} would like to access your memory card."
msgstr "{{app}} na son ya sami dama ga katinka mai ƙwaƙwalwa."

#: perm-device-storage-sdcard-webRequest
msgid "{{site}} would like to access your memory card."
msgstr "{{site}} na son ya sami dama ga katinka mai ƙwaƙwalwa."

#: perm-device-storage-videos
msgid "Video Storage"
msgstr "Ma'ajiyar Bidiyo"

#: perm-device-storage-videos-appRequest
msgid "{{app}} would like to access your videos."
msgstr "{{app}} na neman samun dama ga bidiyoyinka."

#: perm-device-storage-videos-webRequest
