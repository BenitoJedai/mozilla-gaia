msgstr "Zaɓi Kyamara"

#: perm-contacts
msgid "Contacts"
msgstr "Abokan hulɗa"

#: perm-contacts-appRequest
msgid "{{app}} would like to access your contact list."
msgstr "{{app}} na son ya sami dama ga jerin abokan hulɗarka."

#: perm-contacts-webRequest
msgid "{{site}} would like to access your contact list."
msgstr "{{site}} na son ya sami dama ga jerin abokan hulɗarka."

