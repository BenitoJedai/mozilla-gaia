msgid "Self-Timer"
msgstr "சுய-கடிகாரம்"

#: self-timer-header
msgid "{{self-timer}}"
msgstr "{{self-timer}}"

#: self-timer-off
msgctxt "self-timer-off"
msgid "Off"
msgstr "முடக்கு"

#: self-timer-2-seconds
msgid "2 seconds"
msgstr "2 விநாடிகள்"

#: self-timer-5-seconds
msgid "5 seconds"
msgstr "5 விநாடிகள்"

#: self-timer-10-seconds
msgid "10 seconds"
msgstr "10 விநாடிகள்"

#: battery-shutdown-title
msgid "Low Battery"
msgstr "குறைந்த மின்கலம்"

#: battery-shutdown-text
msgid "The battery is too low to use the camera."
msgstr "புகைப்பட கருவியைப் பயன்படுத்த மிகவும் குறைவான மின்கலனே உள்ளது"

#: battery-critical-text
msgid "Critically low battery"
msgstr "எச்சரிக்கை மிகவும் குறைவான மின்கலம்"

#: battery-verylow-text
msgid "10% battery remaining"
msgstr "10% மின்சக்தி மீதமுள்ளது"

#: battery-low-text
msgid "15% battery remaining"
msgstr "15% மின்சக்தி மீதமுள்ளது"

