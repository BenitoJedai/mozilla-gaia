#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "I-Bluetooth"

#: confirmation
msgid "Confirmation"
msgstr "Isiqinisekiso"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "I-Bluetooth icishiwe"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Uyafuna ukuvula i-Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Khansela"

#: turn-on
msgid "Turn On"
msgstr "Vula"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Ukuthunyelwa kwefayela nge-Bluetooth akuphumelelanga"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Ukuthunyelwa kwefayela nge-Bluetooth akuphumelelanga. Hlolisisa ukuthi "
"amasethingi e-Bluetooth asesimeni."

#: ok
msgid "OK"
msgstr "KULUNGILE"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "Amadivayisi ahambisanayo"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "Amadivayisi akule ndawo"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Thinta kancane ukuze uxhumeke"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Isazama ukuxhumana nedivayisi..."

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Ilindele enye idivayisi..."

#: device-status-paired
msgid "Paired"
msgstr "Asexhumene"

#: device-status-connecting
msgid "Connecting…"
msgstr "Isaxhuma..."

#: device-status-connected
msgid "Connected"
msgstr "Ixhumekile"

#: search-for-device
msgid "Searching for devices…"
msgstr "Isacinga amadivayisi..."

#: search-device
msgid "Search for devices"
msgstr "Cinga amadivayisi"

#: unnamed-device
msgid "Unknown device"
msgstr "Idivayisi engaziwa"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Amadivayisi akavumi ukuhambisana"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Ayikwazi ukuhambisana nedivayisi. Hlola ukuthi i-PIN ilungile na."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Ayikwazi ukuhambisana nedivayisi. Kunezicelo eziningi kakhulu."
