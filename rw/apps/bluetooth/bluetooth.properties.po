#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "Burutufu"

#: confirmation
msgid "Confirmation"
msgstr "Icyemezo"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "Burutufu irazimije"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Urashaka gufungura Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Bireke"

#: turn-on
msgid "Turn On"
msgstr "Burutufu ntago iriho"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Kohereza na burutufu byanze"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Kohereza na burutufu byanze. Suzuma niba igenamiterere rya Burutufu ari "
"rizima."

#: ok
msgid "OK"
msgstr "YEGO"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "Ihuza ry'ibikoresho"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "Ibikoresho biri hafi aho"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Kanda kugirango wihuze"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Guhuza n'ikindi gikoresho..."

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Itegereza ry'ikindi gikoresho..."

#: device-status-paired
msgid "Paired"
msgstr "Byahujwe"

#: device-status-connecting
msgid "Connecting…"
msgstr "Guhuza..."

#: device-status-connected
msgid "Connected"
msgstr "Byahujwe"

#: search-for-device
msgid "Searching for devices…"
msgstr "Gushakisha ibikoresho..."

#: search-device
msgid "Search for devices"
msgstr "Ishakisha ry' ibikoresho"

#: unnamed-device
msgid "Unknown device"
msgstr "Iigikoresho kitazwi"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Ntibishobotse guhuza ibikoresho"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Ntibishobotse guhuza igikoresho. Reba ko PIN ariyo."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Ntibishobotse guhuza n'igikoresho. Ibisabwa nibyinshi."

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Huza"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Emeza ibikoresho kugirango bihuzwe"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"Guhuza na {{igikoresho}}, banza umenye niba ibikoresho byombi bihari bifite "
"PIN zigaragara:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "Guhuza na {{igikoresho}}, shyiramo PIN mu gikoresho."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "Guhuza n' {{igikoresho}}, shyiramo urufunguzo mu gikoresho."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Gusaba guhuza Bluetooth"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"{{igikoresho}} kirifuza guhuzwa na telefone. Kwemeza, reba neza ko "
"ibikoresho byombi bihari bifite PIN zigaragara ahakurikira:"

#: passive-pair-pincode
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"{{igikoresho}} kirifuza guhuzwa n'iyi telefone. Kwemeza, injiza PIN mu "
"gikoresho."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"{{igikoresho}} kirifuza guhuza niyi telefone. Kwemeza, injiza umubare "
"w'ibanga ku gikoresho."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Isaba rya Burutufu rivuye kuri "

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Isaba ry'ihuza rya burutufu"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Isaba ry' ihuza rya burutufu"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr "Isaba ry'ihuza rya bluetooth ryararangiye. Gerageza guhuza nanone."
