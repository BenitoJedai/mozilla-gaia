#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "I-Bluetooth"

#: confirmation
msgid "Confirmation"
msgstr "Isiqinisekiso"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "I-Bluetooth ivaliwe"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Ingaba ufuna ukulayita i-Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Rhoxisa"

#: turn-on
msgid "Turn On"
msgstr "Layita"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Ukuhanjiswa kwefayile nge-Bluetooth kusilele"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Ukuhanjiswa kwefayile nge-Bluetooth kusilele. Hlola iisetingi ze-Bluetooth "
"ukuze zichanile na."

#: ok
msgid "OK"
msgstr "KULUNGILE"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "Izixhobo ezingqamaneyo"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "Izixhobo ezikule ndawo"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Cofa ukuze unxibelelanise"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Isangqamana nesixhobo…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Ilindele esinye isixhobo…"

#: device-status-paired
msgid "Paired"
msgstr "Zingqamene"

#: device-status-connecting
msgid "Connecting…"
msgstr "Iyanxibelelana…"

#: device-status-connected
msgid "Connected"
msgstr "Inxityelelwe"

#: search-for-device
msgid "Searching for devices…"
msgstr "Ikhangela izixhobo…"

#: search-device
msgid "Search for devices"
msgstr "Khangela izixhobo"

#: unnamed-device
msgid "Unknown device"
msgstr "Izixhobo ezingaziwayo"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Ayikwazi ukungqamanisa izixhobo"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Ayikwazi ukungqamana nesixhobo. Hlola ukuba i-PIN ichanile na."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Ayikwazi ukungqamana nesixhobo. Zininzi gqitha izicelo."
