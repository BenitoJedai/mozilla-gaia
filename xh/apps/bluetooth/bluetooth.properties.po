#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "I-Bluetooth"

#: confirmation
msgid "Confirmation"
msgstr "Isiqinisekiso"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "I-Bluetooth ivaliwe"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Ingaba ufuna ukulayita i-Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Rhoxisa"

#: turn-on
msgid "Turn On"
msgstr "Layita"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Ukuhanjiswa kwefayile nge-Bluetooth kusilele"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr "Iisethingi"

#: ok
msgid "OK"
msgstr "Kulungile"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "Izixhobo ezingqamaneyo"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "Izixhobo ezikule ndawo"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Cofa ukuze unxibelelane"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Isangqamana nesixhobo…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Ilindele esinye isixhobo…"

#: device-status-paired
msgid "Paired"
msgstr "Zingqamene"

#: device-status-connecting
msgid "Connecting…"
msgstr "Iyanxibelelana…"

#: device-status-connected
msgid "Connected"
msgstr "Inxityelelwe"

#: search-for-device
msgid "Searching for devices…"
msgstr "Ikhangela izixhobo…"

#: search-device
msgid "Search for devices"
msgstr "Khangela izixhobo"

#: unnamed-device
msgid "Unknown device"
msgstr "Izixhobo ezingaziwayo"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Ayikwazi ukungqamanisa izixhobo"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Ayikwazi ukungqamana nesixhobo. Hlola ukuba i-PIN ichanile na."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Ayikwazi ukungqamana nesixhobo. Zininzi gqitha izicelo."

#. # Bluetooth :: Pair View
#: pair
msgid "Pair"
msgstr "Hambisa kunye"

#: active-pair
msgid "Confirm devices to pair"
msgstr "Qinisekisa ngezixhobo ezihamba kunye"

#: active-pair-confirmation
msgid ""
"To pair with {{device}}, make sure that both devices currently have the "
"following PIN displayed:"
msgstr ""
"Ukuhambisa ne{{device}}, qinisekisa ukuba zozibini izixhobo zinale-PIN "
"ilandelayo ebonisiweyo:"

#: active-pair-pincode
msgid "To pair with {{device}}, enter the PIN on the device."
msgstr "Ukuhambisa ne{{device}}, faka i-PIN kwisixhobo."

#: active-pair-passkey
msgid "To pair with {{device}}, enter the passkey on the device."
msgstr "Ukuhambisa ne{{device}}, faka ipasskey kwisixhobo."

#: passive-pair
msgctxt "passive-pair"
msgid "Bluetooth pairing request"
msgstr "Isicelo sokuhambisa kunye seBluetooth"

#: passive-pair-confirmation
msgid ""
"{{device}} would like to pair with this phone. To accept, make sure that "
"both devices currently have the following PIN displayed:"
msgstr ""
"I{{device}} ifuna ukuhamba kunye nale fowuni. Ukwamkela oku, qinisekisa "
"ukuba zozibini izixhobo zinale PIN ilandelayo ebonisiweyo:"

#: passive-pair-pincode
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the PIN on "
"the device."
msgstr ""
"I{{device}} ifuna ukuhamba kunye nale fowuni. Ukwamkela oku, faka i-PIN "
"kwisixhobo."

#: passive-pair-passkey
msgid ""
"{{device}} would like to pair with this phone. To accept, enter the passkey "
"on the device."
msgstr ""
"I{{device}} ifuna ukuhamba kunye nale fowuni. Ukwamkela oku, faka ipasskey "
"kwisixhobo."

#: pair-view-title
msgid "Bluetooth Request From"
msgstr "Isicelo seBluetooth sisuka"

#. # Bluetooth :: Pair Manager
#: bluetooth-pairing-request-now-title
msgctxt "bluetooth-pairing-request-now-title"
msgid "Bluetooth pairing request"
msgstr "Isicelo sokuhambisa kunye seBluetooth"

#: pairing-request-timeout-title
msgid "Bluetooth Pairing Request"
msgstr "Isicelo sokuhambisa kunye seBluetooth"

#: pairing-request-timeout-description
msgid "Bluetooth pairing request expired. Try to pair again."
msgstr ""
"Isicelo sokuhambisa kunye seBluetooth siphelelwe. Zama ukuhambisa kunye "
"kwakhona."
