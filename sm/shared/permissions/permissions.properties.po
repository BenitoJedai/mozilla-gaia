#: perm-contacts
msgid "Contacts"
msgstr "Contacts"

#: perm-contacts-appRequest
msgid "{{app}} would like to access your contact list."
msgstr "E fia fa'aaogā e le {{app}} lau lisi tagata-feso'ota'i."

#: perm-contacts-webRequest
msgid "{{site}} would like to access your contact list."
msgstr "E fia fa'aaogā e le {{site}} lau lisi tagata-feso'ota'i."

