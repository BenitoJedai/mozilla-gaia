#. # Bluetooth :: Confirmation Dialog
#: bluetooth
msgid "Bluetooth"
msgstr "Bluetooth"

#: confirmation
msgid "Confirmation"
msgstr "Xác nhận"

#: bluetooth-status-description
msgid "Bluetooth is disabled"
msgstr "Bluetooth đã tắt"

#: turn-bluetooth-on
msgid "Do you want to turn Bluetooth on?"
msgstr "Bạn có muốn bật Bluetooth?"

#: cancel
msgid "Cancel"
msgstr "Hủy"

#: turn-on
msgid "Turn On"
msgstr "Bật"

#: error-transfer-title
msgid "Bluetooth file transfer failed"
msgstr "Chuyển tệp qua Bluetooth thất bại"

#: error-transfer-settings
msgid ""
"Bluetooth file transfer failed. Check that the Bluetooth settings are "
"correct."
msgstr ""
"Chuyển tệp qua bluetooth thất bại. Hãy kiểm tra lại thiết lập Bluetooth."

#: ok
msgid "OK"
msgstr "Đồng ý"

#. # Bluetooth :: Devices Panel
#: bluetooth-paired-devices
msgid "Paired devices"
msgstr "Thiết bị đã kết cặp"

#: bluetooth-devices-in-area
msgid "Devices in the area"
msgstr "Thiết bị quanh đây"

#: device-status-tap-connect
msgid "Tap to connect"
msgstr "Chạm để kết nối"

#: device-status-pairing
msgid "Pairing with device…"
msgstr "Đang kết cặp với thiết bị…"

#: device-status-waiting
msgid "Waiting for other device…"
msgstr "Đang đợi thiết bị còn lại…"

#: device-status-paired
msgid "Paired"
msgstr "Đã kết cặp"

#: device-status-connecting
msgid "Connecting…"
msgstr "Đang kết nối…"

#: device-status-connected
msgid "Connected"
msgstr "Đã kết nối"

#: search-for-device
msgid "Searching for devices…"
msgstr "Đang tìm kiếm thiết bị…"

#: search-device
msgid "Search for devices"
msgstr "Tìm kiếm các thiết bị khác"

#: unnamed-device
msgid "Unknown device"
msgstr "Thiết bị chưa biết"

#: error-pair-title
msgid "Unable to pair devices"
msgstr "Các thiết bị không thể kết cặp"

#: error-pair-pincode
msgid "Unable to pair with the device. Check that the PIN is correct."
msgstr "Không thể kết nối với thiết bị này. Hãy kiểm tra mã PIN."

#: error-pair-toofast
msgid "Unable to pair with the device. Too many requests."
msgstr "Không thể kết cặp với thiết bị này. Có quá nhiều yêu cầu."

