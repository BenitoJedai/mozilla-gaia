#. # Add bookmark to homescreen
#: add-to-home-screen
msgid "Add to Home Screen"
msgstr "បន្ថែមទៅអេក្រង់ដើម"

#: website-name
msgid "Website name"
msgstr "ឈ្មោះ​តំបន់បណ្ដាញ"

#: address
msgid "Address"
msgstr "អាសយដ្ឋាន"

#. # Add app to homescreen from Ev.me
#: add-to-home-screen-question
msgid "Add {{name}} to Home Screen?"
msgstr "បន្ថែម {{name}} ទៅ​អេក្រង់​ដើម?"

#: app-exists-in-home-screen
msgid "{{name}} was already added to Home Screen"
msgstr "{{name}} បាន​បន្ថែម​រួច​ហើយ​ទៅ​អេក្រង់​ដើម"

#. # Wrapper
#: wrapper-search-name
msgid "{{topic}} on {{name}}"
msgstr "{{topic}} លើ {{name}}"

#. # Prompt that appears when a user tries to delete an app.
#: delete-title
msgid "Delete {{name}}"
msgstr "លុប {{name}}"

#: delete-body
msgid "{{name}} and all of its data will be deleted."
msgstr "{{name}} និង​ទិន្នន័យ​ទាំងអស់​របស់​វា​នឹង​ត្រូវ​លុប។"

#: delete
msgid "Delete"
msgstr "លុប"

#: cancel
msgid "Cancel"
msgstr "បោះបង់"

#. # Prompt that appears when a user tries to remove a site saved to the homescreen.
#: remove-title-2
msgid "Remove {{name}}"
msgstr "យក​ចេញ {{name}}"

#: remove-body
msgid "Remove {{name}} from the homescreen?"
msgstr "យក​ចេញ {{name}} ពី​អេក្រង់​ដើម​ឬ?"

#: remove
msgid "Remove"
msgstr "យក​ចេញ"

#. # Evme
#: loadingEvme2
msgid "Loading…"
msgstr "កំពុង​ផ្ទុក..."

#. # Landing
#: longDateFormat
msgid "%A, %B %e"
msgstr "%A, %B %e"

#. # App reinstall
#: download
msgid "Download"
msgstr "ទាញ​យក"

#: restart-download-title
msgid "Restart Download"
msgstr "ចាប់ផ្ដើម​ទាញយក​ឡើ​ងវិញ"

#: restart-download-body
msgid "Do you want to download {{name}}?"
msgstr "តើ​អ្នក​ចង់​ទាញ​យក {{name}}​ឬ?"
